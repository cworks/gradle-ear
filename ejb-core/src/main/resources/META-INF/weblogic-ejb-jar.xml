<weblogic-ejb-jar xmlns="http://www.bea.com/ns/weblogic/weblogic-ejb-jar" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bea.com/ns/weblogic/weblogic-ejb-jar http://www.bea.com/ns/weblogic/weblogic-ejb-jar/1.0/weblogic-ejb-jar.xsd">
  
  <weblogic-enterprise-bean> 
    <ejb-name>ReportServerAdHocRequestHandlerBean</ejb-name>

    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>10</max-beans-in-free-pool>
      </pool>
     <destination-jndi-name>com.nhin.jms.queue.ReportServerAdhocQueue</destination-jndi-name>
    </message-driven-descriptor>
    <dispatch-policy>arv2.reportserver.adhoc</dispatch-policy>
  </weblogic-enterprise-bean>
  
  <weblogic-enterprise-bean>
    <ejb-name>ReportServerReportSetRequestHandlerBean</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>10</max-beans-in-free-pool>
      </pool>
      <destination-jndi-name>com.nhin.jms.queue.ReportServerReportSetQueue</destination-jndi-name>
    </message-driven-descriptor>
    <dispatch-policy>arv2.reportserver.reportset</dispatch-policy>
  </weblogic-enterprise-bean>
  
  <weblogic-enterprise-bean>
    <ejb-name>ReportServerLongRunningRequestHandlerBean</ejb-name>
    <message-driven-descriptor>
      <pool>
        <max-beans-in-free-pool>10</max-beans-in-free-pool>
      </pool>
      <destination-jndi-name>com.nhin.jms.queue.ReportServerLongRunningQueue</destination-jndi-name>
    </message-driven-descriptor>
    <dispatch-policy>arv2.reportserver.longrunning</dispatch-policy>
  </weblogic-enterprise-bean>
  
  <weblogic-enterprise-bean>
    <ejb-name>ReportServerService</ejb-name>
    <enable-call-by-reference>true</enable-call-by-reference>
    <dispatch-policy>arv2.reportserver.reportset</dispatch-policy>
  </weblogic-enterprise-bean>
  
  <weblogic-compatibility>
    <entity-always-uses-transaction>true</entity-always-uses-transaction>
  </weblogic-compatibility>
</weblogic-ejb-jar>
